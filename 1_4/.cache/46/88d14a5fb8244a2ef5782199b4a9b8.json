:0},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":4}},{"generated":{"line":83,"column":11},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":11}},{"name":"PotentialRegression","generated":{"line":83,"column":15},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":15}},{"generated":{"line":83,"column":34},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":11}},{"generated":{"line":83,"column":35},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":35}},{"generated":{"line":83,"column":39},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":11}},{"name":"json","generated":{"line":83,"column":41},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":41}},{"generated":{"line":83,"column":45},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":11}},{"generated":{"line":83,"column":46},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":75,"column":4}},{"generated":{"line":84,"column":0},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":76,"column":3}},{"generated":{"line":86,"column":0},"source":"node_modules/ml-regression/src/regression/potential-regression.js","original":{"line":14,"column":64}}],"sources":{"node_modules/ml-regression/src/regression/potential-regression.js":"import BaseRegression, { maybeToPrecision } from 'ml-regression-base';\nimport PolynomialRegression from 'ml-regression-polynomial';\n\n\n/*\n * Function that calculate the potential fit in the form f(x) = A*x^M\n * with a given M and return de A coefficient.\n *\n * @param {Vector} X - Vector of the x positions of the points.\n * @param {Vector} Y - Vector of the x positions of the points.\n * @param {Number} M - The exponent of the potential fit.\n * @return {Number} A - The A coefficient of the potential fit.\n */\nexport default class PotentialRegression extends BaseRegression {\n  /**\n   * @constructor\n   * @param x: Independent variable\n   * @param y: Dependent variable\n   * @param M\n   */\n  constructor(x, y, M) {\n    super();\n    if (x === true) {\n      // reloading model\n      this.A = y.A;\n      this.M = y.M;\n    } else {\n      var n = x.length;\n      if (n !== y.length) {\n        throw new RangeError('input and output array have a different length');\n      }\n\n      var linear = new PolynomialRegression(x, y, [M]);\n      this.A = linear.coefficients[0];\n      this.M = M;\n    }\n  }\n\n  _predict(x) {\n    return this.A * Math.pow(x, this.M);\n  }\n\n  toJSON() {\n    return {\n      name: 'potentialRegression',\n      A: this.A,\n      M: this.M\n    };\n  }\n\n  toString(precision) {\n    return `f(x) = ${maybeToPrecision(this.A, precision)} * x^${this.M}`;\n  }\n\n  toLaTeX(precision) {\n    if (this.M >= 0) {\n      return (\n        `f(x) = ${maybeToPrecision(this.A, precision)}x^{${this.M}}`\n      );\n    } else {\n      return (\n        `f(x) = \\\\frac{${\n          maybeToPrecision(this.A, precision)\n        }}{x^{${\n          -this.M\n        }}}`\n      );\n    }\n  }\n\n  static load(json) {\n    if (json.name !== 'potentialRegression') {\n      throw new TypeError('not a potential regression model');\n    }\n    return new PotentialRegression(true, json);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"46a661fa591cb5631b59ec3bdb03e880","cacheData":{"env":{}}}